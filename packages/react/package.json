{
  "name": "@suspensive/react",
  "version": "1.11.1",
  "keywords": [
    "suspensive",
    "react"
  ],
  "homepage": "https://suspensive.org",
  "bugs": "https://github.com/suspensive/react/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/suspensive/react.git",
    "directory": "packages/react"
  },
  "license": "MIT",
  "sideEffects": false,
  "exports": {
    ".": {
      "require": "./src/index.ts",
      "import": "./src/index.ts"
    },
    "./package.json": "./package.json"
  },
  "main": "./src/index.ts",
  "files": [
    "dist",
    "esm"
  ],
  "scripts": {
    "build": "rm -rf dist esm && tsc -p tsconfig.build.json --declaration --emitDeclarationOnly --declarationDir dist && rollup -c rollup.config.js",
    "lint": "eslint \"**/*.ts*\"",
    "prepack": "pnpm build",
    "test": "jest",
    "test:watchAll": "jest --watchAll",
    "type:check": "tsc --noEmit -p tsconfig.build.json"
  },
  "devDependencies": {
    "@suspensive/babel": "workspace:*",
    "@suspensive/eslint-config": "workspace:*",
    "@suspensive/jest": "workspace:*",
    "@suspensive/rollup": "workspace:*",
    "@suspensive/tsconfig": "workspace:*",
    "@testing-library/dom": "^8.20.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.5.1",
    "@types/node": "^18.16.2",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.1",
    "@types/testing-library__jest-dom": "^5.14.5",
    "eslint": "^8.42.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^3.21.0",
    "typescript": "^4.9.5"
  },
  "peerDependencies": {
    "react": "^16.8 || ^17 || ^18"
  },
  "publishConfig": {
    "access": "public",
    "main": "dist/index.js",
    "module": "esm/index.mjs",
    "exports": {
      ".": {
        "require": "./dist/index.js",
        "import": "./esm/index.mjs",
        "types": "./dist/index.d.ts"
      },
      "./package.json": "./package.json"
    },
    "types": "dist/index.d.ts",
    "import": "esm/index.mjs"
  }
}
