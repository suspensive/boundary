{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**", ".docusaurus/**", "build/**"]
    },
    "npm:publish": {
      "dependsOn": ["^build"]
    },
    "lint": {
      "outputs": []
    },
    "lint:pub": {
      "outputs": []
    },
    "test": {
      "outputs": []
    },
    "test:tsd": {
      "outputs": []
    },
    "prepack": {
      "dependsOn": ["^prepack"],
      "cache": false
    },
    "dev": {
      "dependsOn": ["^prepack"],
      "cache": false
    },
    "dev:ko": {
      "cache": false
    },
    "type:check": {}
  },
  "globalEnv": ["NODE_ENV"]
}
